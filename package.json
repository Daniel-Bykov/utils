{
  "name": "@artmizu/utils",
  "type": "module",
  "version": "1.1.3",
  "packageManager": "pnpm@7.12.0",
  "description": "Library of useful utilities, some of them specific to the Russian language",
  "author": "Aleksandr Saburov <hello@artmizu.ru> (http://artmizu.ru/)",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/universal.d.ts",
      "require": "./dist/universal.cjs",
      "import": "./dist/universal.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.mjs"
    }
  },
  "main": "./dist/universal.mjs",
  "module": "./dist/universal.mjs",
  "types": "./dist/universal.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/universal.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "start": "esno src/universal.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.0",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  }
}
